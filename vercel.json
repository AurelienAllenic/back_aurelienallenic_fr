{
  "version": 2,
  "crons": [
    {
      "path": "/api/analytics/cron-aggregate",
       "schedule": "06 20 * * * Europe/Paris"
    }
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}